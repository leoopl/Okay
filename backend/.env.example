# Application Settings
NODE_ENV=development
PORT=3001
API_VERSION=v1
FRONTEND_URL=http://localhost:3000
CORS_ORIGIN=http://localhost:3000

# Database Configuration
DB_HOST=postgres
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=your_secure_password
DB_NAME=okay_mental_health
DB_LOGGING=true

# Security Settings
COOKIE_SECRET=your_cookie_secret_key
SECURE_COOKIES=false  # Set to true in production
THROTTLE_TTL=60  # Time window in seconds
THROTTLE_LIMIT=20  # Max requests per time window

# Auth0 Configuration
AUTH0_DOMAIN=your-tenant.auth0.com
AUTH0_CLIENT_ID=your_client_id_from_auth0
AUTH0_CLIENT_SECRET=your_client_secret_from_auth0
AUTH0_AUDIENCE=https://your-tenant.auth0.com/api/v2/
AUTH0_ISSUER_BASE_URL=https://your-tenant.auth0.com/